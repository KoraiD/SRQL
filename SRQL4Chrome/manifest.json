{
  "manifest_version": 2,
  "name": "SRQL4Chrome",
  "version": "0.4.0",
  "description": "Rollbar Query Language - Visual Builder",
  "permissions": [
    "tabs",
    "notifications",
    "activeTab",
    "https://rollbar.com/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-66K5h+fmj7E+5Z759s9LhR69zp016dM6DFgmdUBhlCM=' 'sha256-iSXVLAT5BlSBaRpELJqSUyN2+VcqAH0I+G1Qgd0V0q8=' 'sha256-wIfg61Jq7wAGELh5RWGIo8EJ1EQ6ilJCQjB2b5OQGBE=' https://unpkg.com/interactjs/dist/interact.min.js https://cdn.rollbar.com/rollbarjs/refs/tags/v2.19.2/rollbar.min.js https://code.jquery.com/jquery-3.5.1.slim.min.js https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css; object-src 'self'",
  "background": {
    "scripts": [
      "scripts/rollbar.js",
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "38": "images/icon76.png",
    "48": "images/icon48.png"
  },
  "browser_action": {
    "default_title": "SRQL"
  }
}